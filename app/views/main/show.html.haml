= render 'header'
.show-page
  %section.item-box-outer
    .item-name
      クルマ
    .item-main-box
      .item-image-box
        .item-image
          -# =image_tag 'vw.jpeg'
      %table.item-deta-outer
        %tbody
          %tr.item-deta-box
            %th.item-deta-box_title
              出品者
            %td.item-deta-box_cell
              %a.sell-name.blue-text
                test
              .face-box
                .face-box_good.clear-text
                  %a=image_tag 'grin-solid.svg',width:16,height:16
                  
                  %span.good-value
                    666
                .face-box_normal.clear-text
                  %a=image_tag 'meh-solid.svg',width:16,height:16
                  %span.normal-value
                    5
                .face-box_bad.clear-text
                  %a=image_tag 'frown-solid.svg',width:16,height:16
                  %span.bad-value
                    55
            -# exhibitor
          %tr.item-deta-box
            %th.item-deta-box_title
              カテゴリー
            %td.item-deta-box_cell
              %a.item-main-categly.blue-text.clear-text
                自動車
              .item-sub-categly
